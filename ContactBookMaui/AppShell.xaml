<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="ContactBookMaui.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:ContactBookMaui"
    
    xmlns:pages="clr-namespace:ContactBookMaui.Pages"
      
    Shell.FlyoutBehavior="Disabled"
    Title="ContactBookMaui">

    <ShellContent Title="Contact Main" Route="ContactMainPage" ContentTemplate="{DataTemplate pages:ContactMainPage}" />
    <ShellContent Title="Contact List" Route="ContactListPage" ContentTemplate="{DataTemplate pages:ContactListPage}" />
    <ShellContent Title="Add Contact" Route="ContactAddPage" ContentTemplate="{DataTemplate pages:ContactAddPage}" />
    <ShellContent Title="Update Contact" Route="ContactUpdatePage" ContentTemplate="{DataTemplate pages:ContactUpdatePage}" />
    <ShellContent Title="Delete Contact" Route="ContactDeletePage" ContentTemplate="{DataTemplate pages:ContactDeletePage}" />


</Shell>
